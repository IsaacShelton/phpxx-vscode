{
    "name": "phpxx-language",
    "displayName": "PHP++/PHPxx language",
    "description": "PHP++/PHPxx language support for VS Code",
    "version": "1.0.0",
    "publisher": "DockySoft",
    "engines": {
        "vscode": "^1.41.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/IsaacShelton/phpxx-vscode/"
    },
    "icon": "icon.png",
    "contributes": {
        "languages": [
            {
                "id": "phpxx",
                "aliases": [
                    "PHP++",
                    "PHPxx"
                ],
                "extensions": [
                    ".phpxx"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "phpxx",
                "scopeName": "source.phpxx",
                "path": "./syntaxes/phpxx.tmLanguage.json"
            }
        ]
    }
}
